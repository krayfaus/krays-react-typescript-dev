{
  "name": "appname",
  "author": "Krayfaus",
  "version": "0.0.1",
  "license": "MIT",
  "description": "This app is build with Electron, React, and TypeScript.",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "dev.krayfaus.appname",
    "productName": "appname",
    "directories": {
      "app": "./dist/",
      "output": "./build/"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "asar": true
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "buildVersion": "1"
  },
  "dependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "electron": "^12.0.4",
    "electron-builder": "^22.10.5",
    "eslint": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-webpack-plugin": "^5.3.1",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0"
  }
}
